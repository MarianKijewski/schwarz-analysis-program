{
  "_version": "1.59.0",
  "sap.app": {
    "id": "orders",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:lrop",
      "version": "1.14.4",
      "toolsId": "71c50f20-b726-4d42-b0e2-e65400e9e25c"
    },
    "dataSources": {
      "mainService": {
        "uri": "/odata/v4/messenger/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "4.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.120.17",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.ushell": {},
        "sap.fe.templates": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "orders.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
      "@i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "uri": "i18n/i18n.properties"
      }
    },
    "resources": {
      "css": []
    },
    "routing": {
      "config": {},
      "routes": [
        {
          "pattern": ":?query:",
          "name": "S_OrdersList",
          "target": "S_OrdersList"
        },
        {
          "pattern": "S_Orders({key}):?query:",
          "name": "S_OrdersObjectPage",
          "target": "S_OrdersObjectPage"
        }
      ],
      "targets": {
        "S_OrdersList": {
          "type": "Component",
          "id": "S_OrdersList",
          "name": "sap.fe.templates.ListReport",
          "options": {
            "settings": {
              "contextPath": "/S_Orders",
              "variantManagement": "Page",
              "navigation": {
                "S_Orders": {
                  "detail": {
                    "route": "S_OrdersObjectPage"
                  }
                }
              },
              "initialLoad": "Enabled",
              "controlConfiguration": {
                "@com.sap.vocabularies.UI.v1.LineItem": {
                  "actions": {
                    "submitOrder": {
                      "press": "orders.ext.controller.SubmitOrder.submitOrder",
                      "visible": true,
                      "enabled": true,
                      "requiresSelection": true,
                      "text": "Submit Order"
                    }
                  }
                }
              }
            }
          }
        },
        "S_OrdersObjectPage": {
          "type": "Component",
          "id": "S_OrdersObjectPage",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "editableHeaderContent": false,
              "contextPath": "/S_Orders",
              "content": {
                "body": {
                  "sections": {
                    "OrderItems": {
                      "template": "sap.fe.templates.ObjectPage.Table",
                      "position": {
                        "placement": "After",
                        "anchor": "Details"
                      },
                      "title": "Order Items",
                      "type": "Default",
                      "id": "OrderItemsSection",
                      "tableSettings": {
                        "type": "ResponsiveTable",
                        "condensedTableLayout": false,
                        "enableExport": true,
                        "selectionMode": "None",
                        "personalization": false
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "extends": {
      "extensions": {
        "sap.ui.controllerExtensions": {
          "sap.fe.templates.ListReport.ListReportController#orders::S_OrdersList": {
            "controllerName": "orders.ext.controller.SubmitOrder"
          }
        }
      }
    }
  },
  "sap.fiori": {
    "registrationIds": [],
    "archeType": "transactional"
  }
}
